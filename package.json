{
  "name": "read-pubspec",
  "version": "0.0.0-development",
  "description": "Read a pubspec.yaml file",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/kirkeaton/read-pubspec/issues"
  },
  "dependencies": {
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@kirkeaton/prettier-config": "1.0.6",
    "@kirkeaton/tsconfig": "2.1.1",
    "@types/node": "24.10.9",
    "prettier": "3.8.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "funding": "https://kirkeaton.ca/sponsor",
  "homepage": "https://github.com/kirkeaton/read-pubspec#readme",
  "keywords": [
    "dart",
    "file",
    "flutter",
    "load",
    "parse",
    "pubspec",
    "read",
    "yaml"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirkeaton/read-pubspec.git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,ts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,ts}\" --check",
    "prepublishOnly": "npm run build",
    "test": "node --test",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
